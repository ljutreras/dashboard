{
  "name": "backend-template",
  "author": "Leonardo Utreras",
  "license": "ISC",
  "version": "1.2.0",
  "description": "first backend template",
  "main": "index.js",
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules",
      "tmpnodejsnpm-cache"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/app/server/Index.ts",
    "ext": "ts, js"
  },
  "scripts": {
    "dev": "nodemon"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "chai": "^4.3.7",
    "cucumber": "^6.0.7",
    "dotenv": "^16.3.1",
    "eslint": "^8.47.0",
    "express": "^4.18.2",
    "mongodb": "^5.8.1",
    "node-dependency-injection": "^3.1.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-jest": "^0.0.1",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.17"
  }
}
